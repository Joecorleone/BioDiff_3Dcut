reset
set encoding utf8
set view map  
set term png
set output '/home/reim/Programmierung/BiodiffPlot/cut04K_hk0.map.png'
set pm3d interpolate 1,1
set palette positive nops_allcF maxcolors 0 gamma 1.5 color model RGB
set palette defined ( 0 0 0 1, 0.25 0 1 1, 0.5 0 1 0, 0.75 1 1 0, 1 1 0 0 )
 set ylabel 'Q_y'
set xlabel 'Q_x'
set colorbox
show colorbox
set cbrange [0.000000:3405.868519]
set title ''
set size square
set border lw 0.6
set tics scale 0.5
splot [:][:][:] '/home/reim/Programmierung/BiodiffPlot/cut04K_hk0.map.map' u 1:2:($3*1) with pm3d notitle